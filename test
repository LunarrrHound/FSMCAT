digraph "State Machine" {
	graph [color=black directed=true fillcolor=white label="State Machine" rankdir=LR "strict"=false style=solid]
	node [color=black fillcolor=white peripheries=1 shape=rectangle style="rounded, filled"]
	edge [color=black]
	asleep [label="asleep\l" color=red fillcolor=darksalmon peripheries=2]
	hungry [label="hungry\l"]
	dirty [label="dirty\l"]
	walking [label="walking\l"]
	savingcat [label="savingcat\l"]
	asleep -> walking [label=wakeup]
	asleep -> savingcat [label=savecat]
	asleep -> asleep [label=nap]
	walking -> hungry [label=run]
	walking -> savingcat [label=savecat]
	walking -> asleep [label=nap]
	hungry -> walking [label=eat]
	hungry -> savingcat [label=savecat]
	hungry -> asleep [label=nap]
	dirty -> savingcat [label=savecat]
	dirty -> asleep [label="clean | nap"]
	dirty -> walking [label=clean]
	savingcat -> savingcat [label=savecat]
	savingcat -> dirty [label=done]
	savingcat -> asleep [label=nap]
}
